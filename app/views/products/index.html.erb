<table>
  <caption>Products</caption>
  <tr>
    <th>Product id</th>
    <th>Description</th>
    <th>List Price</th>
    <th>Shipping</th>
  </tr>
  <% @products.each do |product| %>
  <tr>
    <td><%=product.product_id%></td>
    <td><%=product.description%></td>
    <td><%=product.list_price%></td>
    <td><%=product.shipping%></td>
  </tr>
  <% end %>
</table>
